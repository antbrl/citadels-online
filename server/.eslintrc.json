{
  "extends": [
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "socket"
        ]
      }
    ],
    "no-console": 0,
    "lines-between-class-members": "off",
    "@typescript-eslint/lines-between-class-members": ["off"]
  }
}
